@{
  Layout = "_Layout";
}

<div class="jumbotron text-center">
  <h1>Address Book</h1>
</div>
<div class="well text-center">
  <h3>Welcome to your Address Book!</h3><br>
  <h3><u>All Contacts</u></h3>
  @foreach (var contact in Model)
  {
    <p><a href="/contact/@contact.GetId()">@contact.GetName()</a><p>
  }
  <h4><a href="/contact/new">Add New Contact</a></h4>
  <h4><a href="/contact/clear">Clear All Contacts</a></h4>
</div>
